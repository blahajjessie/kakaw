{
	"main": "dist/server.js",
	"dependencies": {
		"@types/cors": "^2.8.13",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.15",
		"@types/jest": "^29.5.0",
		"@types/supertest": "^2.0.12",
		"@types/ws": "^8.5.4",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.3",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"typescript": "^4.9.4"
	},
	"engines": {
		"node": "^18.13.0"
	},
	"scripts": {
		"test": "jest --runInBand --coverage --forceExit --detectOpenHandles --silent",
		"test-verbose": "jest --runInBand --coverage --forceExit --detectOpenHandles",
		"format": "prettier -w src",
		"check-format": "prettier -c src"
	},
	"jest": {
		"testTimeout": 15000,
		"clearMocks": true,
		"collectCoverageFrom": [
			"**/src/{!(__tests__),}"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"transform": {
			"^.+\\.(ts|tsx)$": "ts-jest"
		},
		"testMatch": [
			"**/src/__tests__/*.+(ts|tsx|js)"
		]
	}
}
